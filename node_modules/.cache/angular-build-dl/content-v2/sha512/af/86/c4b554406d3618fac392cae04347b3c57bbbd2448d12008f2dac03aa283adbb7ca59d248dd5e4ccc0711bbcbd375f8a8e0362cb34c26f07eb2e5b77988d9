            yield this.UserDataService.loadUserProfile();
            //updating the form default value with new value
            this.updateDefault();
        });
    }
    //updating default value
    updateDefault() {
        console.log("udating");
        this.testing.patchValue({
            item1: this.UserDataService.user["item1"] || "Undefined",
            item2: this.UserDataService.user["item2"] || "Undefined",
        });
        console.log(this.UserDataService.user["item1"]);
        console.log(this.UserDataService.user["item2"]);
    }
    // loop and set all values in the DB
    processForm() {
        return Object(tslib__WEBPACK_IMPORTED_MODULE_0__["__awaiter"])(this, void 0, void 0, function* () {
