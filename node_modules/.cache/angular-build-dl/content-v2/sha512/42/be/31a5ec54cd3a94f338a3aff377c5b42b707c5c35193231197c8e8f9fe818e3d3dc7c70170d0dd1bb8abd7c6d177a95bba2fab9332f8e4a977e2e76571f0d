//barcode scanner and generator





const { Storage } = _capacitor_core__WEBPACK_IMPORTED_MODULE_8__["Plugins"];
let HomePage = class HomePage {
    constructor(alertController, barcodeScanner, userDataService, LoginService) {
        this.alertController = alertController;
        this.barcodeScanner = barcodeScanner;
        this.userDataService = userDataService;
        this.LoginService = LoginService;
        //Options
        this.barcodeScannerOptions = {
            showTorchButton: true,
            showFlipCameraButton: true
        };
        //login timeout set this for every pasge
        //this.LoginService.timeOut();
    }
    ngOnInit() {
        return Object(tslib__WEBPACK_IMPORTED_MODULE_0__["__awaiter"])(this, void 0, void 0, function* () {
            //load user data 
            yield this.userDataService.loadUserProfile();
            console.log("started app");
        });
    }
    alertScanCode() {
        return Object(tslib__WEBPACK_IMPORTED_MODULE_0__["__awaiter"])(this, void 0, void 0, function* () {
            const alert = yield this.alertController.create({
                header: 'Confirm Update!',
                message: 'The record in the App will be replaced by the values scanned from QR Code',
                buttons: [
                    {
                        text: 'Cancel',
                        role: 'cancel',
                        cssClass: 'secondary',
                        handler: (blah) => {
                            console.log('Confirm Cancel: blah');
                        }
                    }, {
                        text: 'Confirm',
                        handler: (blah) => {
                            this.scanCode();
                        }
                    }
                ]
            });
            yield alert.present();
        });
    }
    //scan qr code
    scanCode() {
        return Object(tslib__WEBPACK_IMPORTED_MODULE_0__["__awaiter"])(this, void 0, void 0, function* () {
            //await this.LoginService.stopPauseMonitor();
            yield this.barcodeScanner
                .scan()
                .then(barcodeData => {
                console.log(barcodeData);
                //alert("Barcode data " + JSON.stringify(barcodeData));
                //extracting the text scanned 
                this.scannedData = JSON.parse(barcodeData["text"]);
                //for each data in the JSON Scanned
                Object.keys(this.scannedData["userProfile"]).forEach(key => {
                    //assign value base on key
                    this.userDataService.userProfileValue[key] = this.scannedData["userProfile"][key];
                    //console.log(key, this.UserDataService.user[key])
                });
                this.userDataService.saveUserProfile();
                Object.keys(this.scannedData["medicalRecord"]).forEach(key => {
                    //assign value base on key
                    this.userDataService.medicalRecordValue[key] = this.scannedData["medicalRecord"][key];
                    //console.log(key, this.UserDataService.user[key])
                });
                this.userDataService.saveMedicalRecord();
            })
                .catch(err => {
                console.log("Error", err);
            });
            //await this.LoginService.startPauseMonitor();
        });
    }
    //encode all data fro current user to qr code
    encodData() {
        return Object(tslib__WEBPACK_IMPORTED_MODULE_0__["__awaiter"])(this, void 0, void 0, function* () {
            //await this.LoginService.stopPauseMonitor();
            this.userDataService.convertToString();
            yield this.barcodeScanner
                .encode(this.barcodeScanner.Encode.TEXT_TYPE, this.userDataService.userProfileString)
                .then(encodedData => {
                console.log(encodedData);
                this.encodeData = encodedData;
            }, err => {
                console.log("Error occured : " + err);
            });
            //await this.LoginService.startPauseMonitor();
        });
    }
    //reset storage
    resetStorage() {
        Storage.clear();
        console.log("Storage Cleared");
    }
};
