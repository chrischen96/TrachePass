                relationship: this.userDataService.user["relationship"] || "",
                tubeType: this.userDataService.user["tubeType"] || "",
                subType: this.userDataService.user["subType"] || "",
                sizeID: this.userDataService.user["sizeID"] || "",
                shaftLength: this.userDataService.user["shaftLength"] || "",
                suctionLength: this.userDataService.user["suctionLength"] || "",
                notes: this.userDataService.user["notes"] || "",
            });
            if (this.userDataService.user["customize"] == false) {
                console.log("starting");
                this.subtypeList = yield this.tubeTypes[this.userDataService.user["tubeType"]];
                console.log(this.subtypeList);
                this.sizeIDList = yield this.subtypeList[this.userDataService.user["subType"]];
                console.log(this.sizeIDList);
                this.shaftLength = yield this.sizeIDList[this.userDataService.user["sizeID"]];
                console.log(this.shaftLength);
            }
        });
    }
