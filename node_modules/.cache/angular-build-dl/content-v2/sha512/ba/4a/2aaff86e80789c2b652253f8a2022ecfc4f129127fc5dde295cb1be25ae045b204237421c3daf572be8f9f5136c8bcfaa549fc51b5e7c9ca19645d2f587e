            });
        });
    }
    selectedTubeType() {
        //console.log(this.tubeTypes[this.editInfo.value.tubeType]);
        console.log(this.editInfo.value.tubeType);
        this.subtypeList = this.tubeTypes[this.editInfo.value.tubeType];
    }
    selectedSubTubeType() {
        console.log(this.subtypeList);
        console.log(this.editInfo.value.subType);
        this.sizeIDList = this.subtypeList[this.editInfo.value.subType];
    }
    selectedSize() {
        this.shaftLength = this.sizeIDList[this.editInfo.value.sizeID];
    }
