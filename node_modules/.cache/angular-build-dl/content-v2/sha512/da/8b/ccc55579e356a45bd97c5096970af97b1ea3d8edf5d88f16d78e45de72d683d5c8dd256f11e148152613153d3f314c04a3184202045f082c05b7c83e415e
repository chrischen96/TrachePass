rachealTube = this.formBuilder.group({\n      //use different valaditator\n      SizeID:['' , ],\n      OD:['', ],\n    });\n  }\n  \n  async ngOnInit() {\n    //load user data must await!!\n    await this.UserDataService.loadUserProfile();\n    //updating the form default value with new value\n    this.updateDefault();\n  }\n\n  //updating default value\n  updateDefault() {\n    console.log(\"udating\");\n    this.EndotrachealTube.patchValue({\n      item1: this.UserDataService.user[\"SizeID\"] || \"\",\n      item2: this.UserDataService.user[\"OD\"] || \"\",\n    })\n    console.log(this.UserDataService.user[\"SizeID\"]);\n    console.log(this.UserDataService.user[\"OD\"]);\n  }\n\n  // loop and set all values in the DB\n  async processForm() {\n    Object.keys(this.EndotrachealTube.controls).forEach(key => {\n      //console.log(\"saving\")\n      //this.saveValue(String(key), String(this.testing.get(key).value));\n      //console.log(String(key), String(this.testing.get(key).value), this.UserDataService.getValue(key));\n      this.UserDataService.user[key] = this.EndotrachealTube.get(key).value;\n      console.log(key, this.EndotrachealTube.get(key).value)\n    });\n    this.UserDataService.saveValue();\n    this.updateDefault();\n    //go ack to the other page\n    this.router.navigate(['form-temp']);\n  }\n}\n","import { NgModule } from '@angular/core';\nimport { Routes, RouterModule } from '@angular/router';\n\nimport { TestFieldDataInputPage } from './test-field-data-input.page';\n\nconst routes: Routes = [\n  {\n    path: '',\n    component: TestFieldDataInputPage\n  }\n];\n\n@NgModule({\n  imports: [RouterModule.forChild(routes)],\n  exports: [RouterModule],\n})\nexport class TestFieldDataInputPageRoutingModule {}\n","export default \"<ion-header>\\n  <ion-toolbar>\\n    <ion-title>test-field-data-input</ion-title>\\n  </ion-toolbar>\\n</ion-header>\\n\\n<ion-content>\\n  <!--This is the group for form conrol remember to add ReactiveFormModule to the impor